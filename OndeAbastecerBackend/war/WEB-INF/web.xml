		<servlet-class>com.sap.appsexperience.PerfilUsuarioServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PerfilUsuario</servlet-name>
		<url-pattern>/ondeabastecer/usuario/reputacao</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CadastrarPosto</servlet-name>
		<servlet-class>com.sap.appsexperience.CadastrarPostoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CadastrarPosto</servlet-name>
		<url-pattern>/ondeabastecer/posto/cadastrar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CarregarDadosANP</servlet-name>
		<servlet-class>com.sap.appsexperience.CarregarDadosANPServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CarregarDadosANP</servlet-name>
		<url-pattern>/carregaranp</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
